<odoo>
	<data noupdate='0'>

<record id="ovpn_member_form" model="ir.ui.view">
	<field name="name">ovpn.member form</field>
	<field name="model">ovpn.member</field>
	<field name="arch" type="xml">
		<form string="">
			<header>
								<button name="download" icon="fa-download" type="object" />
			</header>
			<sheet>
				<div class="oe_title">
					<h1>
						<field name="name" colspan="4" class="oe_inline" />
					</h1>
				</div>
				<group>
					<field name="download_link"/>
					<field name="download_hash_clear_date"/>
				</group>
				<group string="" col="2">
					<field name="site_id" required='1'/>
					<field name="name"  required='1'/>
					<field name="ip_address"  required='1'/>
					<field name="partner_id"/>
					<field name="is_master"/>
					<field name="force_remote"/>
					<newline/>
				</group>
			</sheet>
		</form>
	</field>
</record>	

	</data>
</odoo>
